{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": [
          "https://assets.chucknorris.host/img/avatar/**",
          "https://fcm.googleapis.com/fcm/send/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "pwa-api",
      "urls": [
        "https://fcm.googleapis.com/fcm/send/**",
        "https://api.chucknorris.io/jokes/random",
        "http://localhost:3000/sendNotification",
        "http://localhost:3000/subscription"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "1m"
      }
    }
  ]
}
